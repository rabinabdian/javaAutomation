package HwBasic1;

public class HwBasic1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int myAge=20;
		long mySalary =300000;
		String myAddress= "TLV HASHALOM 105";
		double myHeight= 185.2;
		System.out.println("myAge "+myAge);
		System.out.println("mySalary "+mySalary);
		System.out.println("myAddress "+myAddress);
		System.out.println("myHeight "+myHeight);
		
		 myAge=22;
		 mySalary =3100000;
		 myAddress= "HERZEKIA HASHALOM 105";
		 myHeight= 187.8;
			System.out.println("---------------- AFTER UPDATE ---------------------");

			System.out.println("myAge "+myAge);
			System.out.println("mySalary "+mySalary);
			System.out.println("myAddress "+myAddress);
			System.out.println("myHeight "+myHeight);
	}

}
